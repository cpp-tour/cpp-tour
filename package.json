        "build:p": "webpack -p",
        "build:profile": "webpack --profile --json > stats.json",
        "build:analyze": "webpack-bundle-analyzer stats.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Leandros/cpp-tour.io.git"
    },
    "keywords": [],
    "author": "Douman <douman@gmx.se>",
    "license": "MIT",
    "devDependencies": {
        "codemirror": "5",
        "webpack": "3",
        "webpack-dev-server": "2",
        "purecss": "1",
        "css-loader": "0",
        "postcss-import": "11",
        "postcss-loader": "2",
        "postcss-nesting": "4",
        "autoprefixer": "7",
        "sugarss": "1",
        "stylelint": "8",
        "stylelint-config-standard": "18",
        "html-webpack-plugin": "2",
        "pug": "^2.0.0-rc.4",
        "pug-loader": "2",
        "script-ext-html-webpack-plugin": "1",
        "extract-text-webpack-plugin": "3",
        "file-loader": "1",
        "json5-loader": "1",
        "raw-loader": "0",
        "babel-core": "6",
        "babel-loader": "7",
        "babel-preset-env": "1",
        "eslint": "4",
