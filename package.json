{
    "name": "cpp-tour",
    "version": "1.0.0",
    "private": true,
    "description": "Front-end for CPP-Tour",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "lint": "eslint . --ext .js --ignore-pattern public",
        "start": "webpack-dev-server",
